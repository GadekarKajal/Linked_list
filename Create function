#include<stdio.h>
#include<stdlib.h>
struct node
{
int data;
struct node*next;
};
struct node*create(struct node*list)
{
int i,n;
struct node*newnode,*temp;
printf("enter limit");
scanf("%d",&n);
for(i=0;i<=n;i++)
{
newnode=(struct node*)malloc(sizeof(struct node));
printf("enter value");
scanf("%d",&newnode->data);
newnode->next=null;
if(list==NULL)
{
list=newnode;
temp=newnode;
}
else
{
temp->next=newnode;
temp=newnode;
return list;
}

int main()
{
int ch,num,pos;
struct node*list=NULL;
do
{
printf("\n1-create");
scanf(%d",&ch);
switch(ch)
{
case1:list =create(NULL);
Break;
}
}
while(ch<6);
}
